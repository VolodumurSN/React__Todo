{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "plugins": ["react"],
    "rules": {   
        "react/jsx-uses-react": "error",   
        "react/jsx-uses-vars": "error",
        "semi": "error"
    }
}